{
  "name": "food",
  "version": "1.0.0",
  "description": "foor microservice",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server",
    "server": "nodemon server.js --exec babel-node --presets babel-preset-env",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Madhuka Perera",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "axios": "^0.24.0",
    "babel-node": "^0.0.1-security",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "got": "^11.8.3",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.13",
    "pipeline": "^0.1.3",
    "request": "^2.88.2",
    "stream": "^0.0.2"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15"
  },
  "engines": {
    "node": "16.13.0",
    "npm": "8.1.0"
  }
}
